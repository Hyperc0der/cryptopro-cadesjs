{
  "plugins": [
    "node_modules/better-docs/category",
    "plugins/markdown"
  ],
  "recurseDepth": 10,
  "source": {
    "include": ["./src/enums", "./src/factories", "./src/models", "./src/services"],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "better-docs": {
      "hideGenerator": true
    }
  },
  "opts": {
    "template": "node_modules/better-docs/",
    "encoding": "utf8",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true,
    "verbose": true
  }
}
